{
  "datasets": [
    "/neodc/esacci/sst/data/CDR_v2/ATSR/L3C/v2.1/AATSR/"
  ],
  "filters": {
    "/neodc/esacci/sst/data/CDR_v2/ATSR/L3C/v2.1/AATSR/": [
      {
        "pattern": ".*day.*",
        "realisation": "day"
      },
      {
        "pattern": ".*night.*",
        "realisation": "night"
      }
    ]
  },
  "mappings": {
    "ecv": {
      "GHRSST": "SST"
    },
    "time_coverage_resolution": {
      "P1DT0H00M00S": "day"
    },
    "sensor": {
    },
    "platform": {
    },
    "institution":{
      "ESACCI":"ESACCI_SST"
    }
  },
  "defaults": {
    "ecv": "sea surface temperature"
  }
}