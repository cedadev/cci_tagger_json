{
  "datasets": [
    "/neodc/esacci/sst/data/CDR_v2/Analysis/L4/v2.1/"
  ],
  "filters": {
    "/neodc/esacci/sst/data/CDR_v2/Analysis/L4/v2.1/": [
      {
        "pattern": ".*1-v02.*",
        "realisation": "sst"
      },
      {
        "pattern": ".*1_anomaly-v02.*",
        "realisation": "anomaly"
      }
    ]
  },
  "mappings": {
    "ecv": {
      "GHRSST": "SST"
    },
    "platform": {
      "MetOpA": "Metop-A",
      "NOAA-07":"NOAA-7",
      "NOAA-09":"NOAA-9"
    },
    "merged": {
      "ATSR, AATSR, AVHRR_GAC": "ATSR,ATSR-2, AATSR,AVHRR-2,AVHRR-3"
    },
    "institution":{
      "ESACCI":"ESACCI_SST"
    }
  },
  "defaults": {
    "ecv": "sea surface temperature"
  }
}